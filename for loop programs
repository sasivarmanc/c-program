1)factorial

#include<stdio.h>
int main()
{
    int n,fact=1;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        fact=fact*i;
    }
    printf("%d\n",fact);
return 0;
    
}


2) sum of natural number

#include<stdio.h>
int main()
{
    int n,fact=1;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        fact=fact+i;
    }
    printf("%d\n",fact);
return 0;
    
}

3)sum of even natural number

#include<stdio.h>
int main()
{
    int n,fact=0;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        if(i%2==0)
          fact=fact+i;
    }
    printf("%d\n",fact);
return 0;
    
}

4) sum of odd natural number

#include<stdio.h>
int main()
{
    int n,fact=0;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
        if(i%2!=0)
          fact=fact+i;
    }
    printf("%d\n",fact);
return 0;
    
}


5) prime number

#include<stdio.h>
int main()
{
    int n,count=0;
    printf("Enter the number:");
    scanf("%d",&n);
    for(int i=2;i<n;i++)
    {
        if(n%i==0)
        {
         count++;
         break;
        }
    }
    if( count==0 )
     printf("Prime Number");
    else
     printf("Not a Prime Number");
return 0;
    
}

6)square

#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter the Number:");
    scanf("%d",&a);
    printf("Enter the Number:");
    scanf("%d",&b);
    int c=a,d=a;
    for(int i=1;i<b;i++)
      c=c*d;
    printf("%d",c);
return 0;
    
}


7)

#include<stdio.h>
int main()
{
    int n,count=0;
    printf("Enter the Number:");
    scanf("%d",&n);
    do{
        n=n/10;
        count++;
    }while(n!=0);
    printf("%d",count);
return 0;
    
}

8)

#include<stdio.h>
int main()
{
    int a,b=10;
    printf("Enter the Number:");
    scanf("%d",&a);
    for(int i=1;i<=a;i++)
     printf("%d\n",i*b);
return 0;
    
}
