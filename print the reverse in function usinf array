#include<stdio.h>
int max(int a[],int n)
{
    printf("The Reverse String:");
 
    for(int i=n-1;i>=0;i--)
    {
        printf("%d\t",a[i]);
    }
}
int main()
{
    int a[100],n;
    printf("Enter the no.of Element in an Array: ");
    scanf("%d",&n);
    printf("Enter the Values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int s=max(a,n);
    return 0;
}
