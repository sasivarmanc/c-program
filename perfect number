#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("Enter the number: ");
    scanf("%d",&n);
    for(int i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum+=i;
        }
    }
    if(sum==n)
    {
        printf("It is a perfect number.");
    }
    else
    {
        printf("It is Not a perfect number.");
    }
    return 0;
}
