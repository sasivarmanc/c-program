#include<stdio.h>
int main()
{
    int a=10,b=20;
    if(a>b)
     printf("a is high");
     else if(a<b)
      printf("b is high");
return 0;
}





2)
int main() {
    int a,b;
    printf("Enter the A number:");
    scanf("%d",&a);
    if(a<0){
        printf("The given number is negative");
    }
    else if(a>0){
        printf("The given number is positive");
    }
    else
    {
       printf("The given number is zero"); 
    }
    return 0;
}




3)

#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter the number=");
    scanf("%d",&a);
    b=a%2;
    if(b==0){
        printf("The given number is even .");
    }
    else{
        printf("The given numbe is odd.");
    }
    return 0;
}

4)

#include<stdio.h>
int main()
{
    int a;
    printf("Enter the number=");
    scanf("%d",&a);
    if(a%4==0 || a%400==0 ){
        printf("The given year is leap year .");
    }
    else{
        printf("The given year is non leap year.");
    }
    return 0;
}


5)


#include<stdio.h>
int main()
{
    char M,G;
    int A;
    printf("Enter the Marital State:");
    scanf("%c",&M);
    printf("Enter the Gender:");
    scanf(" %c",&G);
    printf("Enter the Age:");
    scanf("%d",&A);
    if(M=='M')
    {
        printf("Driver is Insured");
    }
    else if(G=='M' && A>=30){
            printf("Driver is Insured");
        }
        
    else if(G=='F' && A>=25){
            printf("Driver is Insured");
        }
        else{
            printf("Driver is Not Insured");
        }
    
    return 0;
}
