    scanf("%d",&n);
    printf("Enter the values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<=n;i++)
    {
       sum=sum+i;    
      
    }
         printf("%d ",sum); 
return 0;
}


5)

#include<stdio.h>
int main()
{
    int arr[5],n,r;
    int start[0];
    printf("Enter the Number:");
    scanf("%d",&n);
    printf("Enter the values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=1;i<=n;i++)
    {
        if(start[0]<i)
        {
            start[0]=i;
            printf("%d",start[0]); 
            }
    }
return 0;
}


6)

#include<stdio.h>
int main()
{
    int a[5],i,j,n,temp;
    
    printf("Enter the Number:");
    scanf("%d",&n);
    printf("Enter the values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
            {
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",a[i]);
    }
     
return 0;
}
