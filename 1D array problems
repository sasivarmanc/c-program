#include<stdio.h>
int main()
{
    int arr[5],n,r;
    printf("Enter the Number:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++)
    {
       printf("%d\n",arr[i]); 
    }
        
return 0;
}



2)

#include<stdio.h>
int main()
{
    int arr[5],n,r;
    printf("Enter the Number:");
    scanf("%d",&n);
    printf("Enter the values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=n-1;i>=0;i--)
    {
       printf("%d ",arr[i]); 
    }
        
return 0;
}    



3)

#include<stdio.h>
int main()
{
    int arr[5],n,r,sum=0;
    printf("Enter the Number:");
    scanf("%d",&n);
    printf("Enter the values: ");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<=n;i++)
    {
       sum=sum+i;    
      
    }
         printf("%d ",sum); 
return 0;
}



